<style>
        li.selectedLine {
            border: 2px solid green;
        }
    </style>
<script>
        if (window.location.hash) {
            if (window.location.hash.indexOf("-") >= 0) {
                var split = window.location.hash.split("-");
                var start = split[0];
                var end = split[1];
                var startNumber = start.match(/\d/g);
                startNumber = startNumber.join("");
                var endNumber = end.match(/\d/g);
                endNumber = endNumber.join("");
                current = startNumber;
                endNumber++;
                $("#LINE" + startNumber).get(0).scrollIntoView();
                while (current < endNumber) {
                    $("#LINE" + current).addClass("selectedLine");
                    current++;
                }
            } else {
                $(window.location.hash).addClass("selectedLine");
            }
        }
        $("a").click(function () {
            var context = $(this).context;
            if (context.hash) {
                $(".selectedLine").removeClass("selectedLine");
                $(context.hash).addClass("selectedLine");
            }
        });
    </script>
